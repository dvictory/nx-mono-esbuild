{
  "name": "next-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/next-app",
  "projectType": "application",
  "targets": {
    "build": {
      "dependsOn": ["clean"],
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "root": "apps/next-app",
        "outputPath": "dist/apps/next-app"
      },
      "configurations": {
        "development": {
          "root": "apps/next-app",
          "outputPath": "apps/next-app/dist"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "next-app:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "next-app:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "next-app:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "next-app:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/next-app/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/next-app/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "dependsOn": [""],
      "options": {
        "engine": "docker",
        "file": "./apps/next-app/ci.Dockerfile",
        "metadata": {
          "images": ["dbd/next-app"],
          "load": true,
          "tags": [
            "type=schedule",
            "type=ref,event=branch",
            "type=sha,prefix=sha-",
            "type=ref,event=tag"
          ]
        }
      }
    },
    "dev": {
      "executor": "@berenddeboer/nx-sst:sst",
      "options": {
        "command": "dev"
      }
    },
    "diff": {
      "executor": "@berenddeboer/nx-sst:sst",
      "options": {
        "command": "diff"
      }
    },
    "foo": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "echo 'foo'"
        ]
      }
    },
    "cleanUpBuild": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "commands": [
          "cd apps/next-app/dist && sed 's@/dist/.next@/.next@g' .next/standalone/apps/next-app/server.js > ./.next/standalone/server.js",
          "cd apps/next-app/dist && cp -a .next/standalone/apps/next-app/dist/.next/. ./.next/standalone/apps/next-app/.next/",
          "cd apps/next-app && mv dist/.next ./"
        ],
        "parallel": false
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf apps/next-app/.sst",
          "rm -rf apps/next-app/.next",
          "rm -rf apps/next-app/dist",
          "rm -rf apps/next-app/.open-next"
        ],
        "parallel": false
      }
    },
    "buildSST": {
      "executor": "@berenddeboer/nx-sst:sst",
      "dependsOn": ["cleanUpBuild"],
      "options": {
        "command": "build"
      },
      "outputs": ["apps/next-app/.sst"]
    },
    "deploy": {
      "executor": "@berenddeboer/nx-sst:sst",
      "dependsOn": ["cleanUpBuild"],
      "options": {
        "command": "deploy"
      }
    },
    "remove": {
      "executor": "@berenddeboer/nx-sst:sst",
      "options": {
        "command": "remove"
      }
    },
    "turbo": {
      "command": "next dev --turbo"
    }
  },
  "tags": []
}
