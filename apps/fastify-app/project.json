{
  "name": "fastify-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/fastify-app/src",
  "projectType": "application",
  "targets": {
    "build-webpack": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/fastify-app",
        "main": "apps/fastify-app/src/main.ts",
        "tsConfig": "apps/fastify-app/tsconfig.app.json",
        "generatePackageJson": true,
        "assets": ["apps/fastify-app/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/fastify-app/src/environments/environment.ts",
              "with": "apps/fastify-app/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "copy-assets": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mkdir -p dist/apps/fastify-app && cp -r apps/fastify-app/src/assets/* dist/apps/fastify-app/assets/*"
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "fastify-app:build",
        "runtimeArgs": ["--enable-source-maps"]
      },
      "configurations": {
        "production": {
          "buildTarget": "fastify-app:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/fastify-app/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/fastify-app/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "dependsOn": ["build-es"],
      "options": {
        "engine": "docker",
        "metadata": {
          "images": ["dbd/fastify-app"],
          "load": true,
          "tags": [
            "type=schedule",
            "type=ref,event=branch",
            "type=ref,event=tag",
            "type=ref,event=pr",
            "type=sha,prefix=sha-"
          ]
        }
      }
    },
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "main": "apps/fastify-app/src/main.ts",
        "outputPath": "dist/apps/fastify-app",
        "outputFileName": "main.mjs",
        "tsConfig": "apps/fastify-app/tsconfig.app.json",
        "bundle": true,
        "splitting": false,
        "assets": [
          {
            "glob": "apps/fastify-app/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "format": ["esm"],
        "platform": "node",
        "target": "esnext",
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".mjs"
          },
          "banner": {
            "js": "import { createRequire } from 'module';const require = createRequire(import.meta.url);"
          }
        },
        "thirdParty": true
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true
        }
      }
    },
    "build-es": {
      "executor": "@wanews/nx-esbuild:build",
      "dependsOn": ["copy-assets"],
      "outputs": ["{options.outdir}"],
      "defaultConfiguration": "development",
      "options": {
        "entryPoints": ["apps/fastify-app/src/main.ts"],
        "outdir": "dist/apps/fastify-app",
        "tsconfig": "apps/fastify-app/tsconfig.app.json",
        "bundle": true,
        "splitting": false,
        "format": "esm",
        "platform": "node",
        "target": "esnext",
        "sourcemap": true,
        "outExtension": {
          ".js": ".mjs"
        },
        "plugins": [
          {
            "package": "esbuild-copy-static-files",
            "args": {
              "src": "apps/fastify-app/src/assets",
              "dest": "dist/apps/fastify-app/assets"
            }
          }
        ],
        "banner": {
          "js": "import { createRequire } from 'module';const require = createRequire(import.meta.url);"
        }
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true
        }
      }
    },
    "serve-es": {
      "executor": "@wanews/nx-esbuild:serve",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "entryPoints": ["apps/fastify-app/src/main.ts"],
        "outfile": "dist/apps/fastify-app/main.mjs",
        "tsconfig": "apps/fastify-app/tsconfig.app.json",
        "bundle": true,
        "splitting": false,
        "format": "esm",
        "platform": "node",
        "target": "esnext",
        "sourcemap": true,
        "outExtension": {
          ".js": ".mjs"
        },
        "plugins": [
          {
            "package": "esbuild-copy-static-files",
            "args": {
              "src": "apps/fastify-app/src/assets/",
              "dest": "dist/apps/fastify-app/assets",
              "recursive": false,
              "force": true
            }
          }
        ],
        "banner": {
          "js": "import { createRequire } from 'module';const require = createRequire(import.meta.url);"
        }
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true
        }
      }
    }
  },
  "tags": []
}
